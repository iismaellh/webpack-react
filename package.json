{
  "name": "webpack-react-boilerplate",
  "version": "4.0.0",
  "description": "Plain Webpack 4 + React Boilerplate with Babel, SASS and lodash",
  "author": "Ismael Herrera",
  "license": "MIT",
  "homepage": "https://github.com/randomtu/webpack-react",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack --progress --mode production --config webpack.config.build.js",
    "local": "webpack-dev-server --open --mode development",
    "heroku-postbuild": "webpack --progress --mode production --config webpack.config.build.js",
    "start": "npm run local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randomtu/webpack-react.git"
  },
  "dependencies": {
    "lodash": "4.17.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.3.0-rc.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "1.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "2.1.2",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "webpack-react": "^0.6.0"
  }
}
